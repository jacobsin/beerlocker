{
  "name": "beerlocker",
  "version": "0.1.0",
  "description": "An example to illustrate what js can do for webapp",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "build:js": "babel-node tools/build.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "npm run test:common -- --reporter progress",
    "test:ci": "npm run test:common -- --reporter mocha-teamcity-reporter",
    "test:common": "npm run test:base -- --recursive \"./test/**/*.spec.{js,coffee}\"",
    "test:base": "cross-env NODE_ENV=test mocha --compilers html:./test/compilers/text.js,coffee:coffee-script/register,js:babel-core/register --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "test:elev": "npm run test:base -- --recursive \"./test/acceptance/elevator/**/*.spec.{js,coffee}\" --reporter progress",
    "test:elev:watch": "npm run test:elev -- --watch"
  },
  "license": "MIT",
  "dependencies": {
    "connect-history-api-fallback": "1.1.0",
    "core-js": "2.4.0",
    "express": "4.14.0",
    "mongoose": "4.6.2",
    "ng-redux": "3.3.4-beta.0",
    "object-assign": "4.0.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "redux": "3.5.0",
    "redux-promise-middleware": "3.0.0",
    "reselect": "2.4.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.9.0",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "browser-sync": "2.11.1",
    "chai": "3.4.1",
    "chai-jq": "0.0.9",
    "cheerio": "0.19.0",
    "coffee-loader": "0.7.2",
    "coffee-script": "1.10.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-react": "4.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "4.5.0",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "ignore-styles": "1.2.0",
    "interfake": "1.19.0",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "8.4.0",
    "less": "2.6.1",
    "less-loader": "2.2.3",
    "lodash": "4.11.1",
    "mocha": "2.3.4",
    "mocha-teamcity-reporter": "1.0.0",
    "ngtemplate-loader": "1.3.1",
    "node-sass": "3.7.0",
    "npm-run-all": "1.6.0",
    "proxy-middleware": "0.15.0",
    "react-addons-test-utils": "0.14.7",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "require-dir": "0.3.0",
    "rimraf": "2.5.0",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "yargs": "3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobsin/beerlocker"
  }
}
